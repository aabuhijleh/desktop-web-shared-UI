{
  "extends": [
    "tslint:latest",
    "tslint-plugin-prettier",
    "tslint-config-prettier"
  ],
  "rules": {
    "prettier": [true, ".prettierrc.json"],
    "max-line-length": [true, 120],
    "no-console": false,
    "no-submodule-imports": [true],
    "curly": true,
    "ordered-imports": true
  }
}
